{
  "name": "jige-ui",
  "type": "module",
  "version": "0.41.0",
  "private": false,
  "description": "a solidjs component library",
  "license": "MIT",
  "sideEffects": false,
  "exports": {
    ".": {
      "types": "./dist/build.d.ts",
      "solid": "./dist/build.jsx",
      "default": "./dist/build.js"
    },
    "./*": {
      "types": "./dist/*/index.d.ts",
      "solid": "./dist/*/index.jsx",
      "default": "./dist/*/index.js"
    }
  },
  "main": "dist/build.js",
  "module": "dist/build.js",
  "types": "dist/build.d.ts",
  "typesVersions": {},
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsdown",
    "build-dev": "tsdown --env.NODE_ENV=development",
    "dev": "vite -c ./playground/vite.config.ts",
    "type-check": "tsc --noEmit --skipLibCheck",
    "bump": "bumpp --no-push -t jige-ui@ -c \"chore(release): jige-ui@\"",
    "prepublish": "pnpm build"
  },
  "peerDependencies": {
    "jige-core": "workspace:^",
    "solid-js": "catalog:"
  },
  "peerDependenciesMeta": {},
  "dependencies": {
    "@solid-primitives/bounds": "catalog:",
    "@solid-primitives/refs": "catalog:",
    "@solid-primitives/scheduled": "catalog:",
    "esday": "^0.5.0",
    "solid-tiny-context": "catalog:",
    "solid-tiny-table": "^0.3.1",
    "solid-tiny-utils": "catalog:"
  },
  "devDependencies": {
    "@iconify-json/fluent": "^1.2.26",
    "@solidjs/router": "^0.15.2",
    "@types/node": "^22.5.2",
    "@jige-ui/unocss": "workspace:^",
    "bumpp": "catalog:",
    "rolldown-plugin-solid": "catalog:",
    "rollup-plugin-inline-import": "^0.3.0",
    "sass-embedded": "^1.91.0",
    "tsdown": "catalog:",
    "typescript": "catalog:",
    "unocss": "^66.5.1",
    "valibot": "1.1.0",
    "vite": "^6.3.5",
    "vite-plugin-solid": "^2.11.8",
    "vite-plugin-solid-pages": "^0.3.5"
  },
  "browser": {
    "./dist/server.js": "./dist/index.js"
  }
}
