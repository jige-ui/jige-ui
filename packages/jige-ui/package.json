{
  "name": "jige-ui",
  "type": "module",
  "version": "0.36.0",
  "private": false,
  "description": "a solidjs component library",
  "license": "MIT",
  "sideEffects": false,
  "exports": {
    ".": {
      "types": "./dist/build.d.ts",
      "solid": "./dist/build.jsx",
      "default": "./dist/build.js"
    },
    "./*": {
      "types": "./dist/*/index.d.ts",
      "solid": "./dist/*/index.jsx",
      "default": "./dist/*/index.js"
    }
  },
  "main": "dist/build.js",
  "module": "dist/build.js",
  "types": "dist/build.d.ts",
  "typesVersions": {},
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup",
    "build-dev": "cross-env DEV_MODE=true tsup",
    "build-down": "tsdown",
    "dev": "vite -c ./playground/vite.config.ts",
    "type-check": "tsc --noEmit --skipLibCheck",
    "bump": "bumpp --no-push -t jige-ui@ -c \"chore(release): jige-ui@\"",
    "prepublish": "pnpm build"
  },
  "peerDependencies": {
    "@tanstack/solid-table": "^8.21.3",
    "jige-core": "workspace:^",
    "solid-js": "catalog:"
  },
  "dependencies": {
    "@solid-primitives/bounds": "catalog:",
    "@solid-primitives/refs": "catalog:",
    "@solid-primitives/scheduled": "catalog:",
    "esday": "^0.5.0",
    "solid-tiny-context": "catalog:",
    "solid-tiny-utils": "catalog:"
  },
  "devDependencies": {
    "@iconify-json/fluent": "^1.2.26",
    "@solidjs/router": "^0.15.2",
    "@types/node": "^22.5.2",
    "browserslist": "^4.25.1",
    "cross-env": "^10.0.0",
    "esbuild-plugin-inline-import": "^1.1.0",
    "esbuild-plugin-solid": "^0.6.0",
    "fast-glob": "^3.3.2",
    "lightningcss": "^1.30.1",
    "rolldown-plugin-solid": "^0.2.1",
    "rollup-plugin-inline-import": "^0.3.0",
    "sass-embedded": "^1.83.0",
    "tsdown": "^0.14.1",
    "tsup": "^8.4.0",
    "typescript": "catalog:",
    "unocss": "^66.0.0",
    "unplugin-icons": "^22.1.0",
    "valibot": "1.1.0",
    "vite": "^6.3.5",
    "vite-plugin-solid": "^2.11.6",
    "vite-plugin-solid-pages": "^0.3.4",
    "bumpp": "catalog:"
  },
  "browser": {
    "./dist/server.js": "./dist/index.js"
  }
}
