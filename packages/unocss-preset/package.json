  "devDependencies": {
    "@types/node": "^24.5.0",
    "tsdown": "catalog:",
    "typescript": "catalog:",
    "bumpp": "catalog:",
    "unocss": "catalog:"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/jige-ui/jige-ui.git",
    "directory": "packages/unocss-preset"
